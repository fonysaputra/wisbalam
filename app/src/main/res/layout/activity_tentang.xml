<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_tentang"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.wisbalam.server.Tentang"
    android:background="#7f0404">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        />

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/card_background1"
        android:layout_centerVertical="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#7f0404"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textVieww"
                    android:layout_width="120dp"
                    android:layout_height="130dp"
                    android:background="@drawable/dinas"
                    android:padding="5dp"
                    android:text=" "
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text=""
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="240dp"
                    android:layout_height="140dp"
                    android:padding="5dp"
                    android:background="@drawable/list1"
                    android:textSize="15sp" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textViews"
                    android:layout_width="230dp"
                    android:layout_height="130dp"
                    android:padding="5dp"
                    android:layout_marginTop="30dp"
                    android:text=""
                    android:background="@drawable/tentangap"
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text=""
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="130dp"
                    android:layout_height="130dp"
                    android:layout_marginTop="30dp"
                    android:padding="5dp"
                    android:background="@drawable/and"
                    android:text=""
                    android:textSize="15sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="120dp"
                    android:layout_marginTop="20dp"
                    android:layout_height="135dp"
                    android:padding="5dp"
                    android:background="@drawable/fotop"
                    android:text=" "
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="5dp"
                    android:text=""
                    android:textSize="15sp" />

                <TextView
                    android:layout_width="230dp"
                    android:layout_height="130dp"
                    android:padding="5dp"
                    android:layout_marginTop="20dp"
                    android:background="@drawable/ijo"
                    android:text=""
                    android:textSize="15sp" />
            </LinearLayout>



        </LinearLayout>
    </FrameLayout>
</RelativeLayout>
